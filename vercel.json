{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/favicon.ico", "dest": "/public/css/$1"" },
    { "src": "/css/(.*)", "dest": "/public/css/$1" },
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
